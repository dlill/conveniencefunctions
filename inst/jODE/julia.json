{
	// Place your snippets for julia here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	"pipe": {
		"prefix": "pipe",
		"body": [ "|> $0"],
		"description": "pipe-operator"
	},

	"cdodo": {
		"prefix": "cdodo",
		"body": [ "# [] "],
		"description": "cdodo"
	},
	"csdodo": {
		"prefix": "csdodo",
		"body": [ "#   [] "],
		"description": "csdodo"
	},
	
	"clist": {
		"prefix": "clist",
		"body": [ "# * "],
		"description": "clist"
	},
	
	"cslist": {
		"prefix": "cslist",
		"body": [ "#   * "],
		"description": "cslist"
	},
	
	"cnlist": {
		"prefix": "cnlist",
		"body": [ "#    "],
		"description": "cnlist"
	},
	"csnlist": {
		"prefix": "csnlist",
		"body": [ "#      "],
		"description": "csnlist"
	},
	"subsectionLIST": {
		"prefix": "subsectionLIST",
        "body": [ "## .. $0 ----", "SUBSEC_$0 = Int64"],
		"description": "subsectionLIST"
	},
	"scriptdevelopment": {
		"prefix": "scriptdevelopment",
		"body": [ "# Script development. # [] Remove in final script ----"],
		"description": "scriptdevelopment"
	},
	"section": {
		"prefix": "section",
		"body": [
			"#-------------------------------------------------------#",
            "## $0 ----",
            "SECTION_$0 = Int64",
			"#-------------------------------------------------------#"
			],
		"description": "Section header"
	}
	
}